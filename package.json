{
  "name": "isncsci",
  "version": "2.0.0-dev",
  "description": "Algorithm designed to produce a spinal cord injury classification consistent with the International Standards for Neurological Classification of Spinal Cord Injury developed and maintained by the American Spinal Injury Association (ASIA)",
  "main": "./dist/cjs/ISNCSCI.js",
  "module": "./dist/esm/ISNCSCI.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "coverage": "jest --coverage",
    "test-result": "jest --json --outputFile=testResult.json",
    "test": "jest && eslint **/*.ts && cspell **",
    "test-silent": "jest --watch --silent"
  },
  "author": "Praxis Spinal Cord Institute",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/jest": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "cspell": "^4.0.61",
    "eslint": "^7.0.0",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.0",
    "rollup-plugin-terser": "^5.2.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
